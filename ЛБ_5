    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "from scipy import optimize\n",
        "\n",
        "# Initial guesses\n",
        "x0 = 0.15\n",
        "y0 = -2.1\n",
        "\n",
        "# Define the functions based on the system\n",
        "def f1(x):\n",
        "    return 1.3 - np.sin(x - 1)\n",
        "\n",
        "def f2(y):\n",
        "    return np.sin(y + 1) + 0.8\n",
        "\n",
        "# Simple Iteration Method\n",
        "def iter_method(x, y, epsilon):\n",
        "    xn, yn = x, y\n",
        "    n = 0\n",
        "    while True:\n",
        "        xn1 = f2(yn)\n",
        "        yn1 = f1(xn)\n",
        "        n += 1\n",
        "        if abs(xn1 - xn) < epsilon and abs(yn1 - yn) < epsilon:\n",
        "            break\n",
        "        xn, yn = xn1, yn1\n",
        "    print('Simple iteration:')\n",
        "    print(f'x = {xn}\\ny = {yn}\\nNumber of iterations = {n}')\n",
        "\n",
        "iter_method(x0, y0, 0.0001)\n",
        "\n",
        "# Verification using scipy's root solver\n",
        "def f3(vars):\n",
        "    x, y = vars\n",
        "    return [np.sin(x - 1) - (1.3 - y), x - np.sin(y + 1) - 0.8]\n",
        "\n",
        "result = optimize.root(f3, [x0, y0], method='hybr')\n",
        "print('Check:', result.x)\n"
      ],
      "metadata": {
